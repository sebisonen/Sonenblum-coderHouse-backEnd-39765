<div >
    <h1>Carts</h1>

    <div id="productsCatalogue">
    {{#each products}}
        <div id="{{this._id}}" class="container">
            <div class="details">
                <div class="content">
                    <h2>{{this.title}} <br>
                    </h2>
                    <p>
                        {{this.description}}
                    </p>
                    <h3>${{this.price}}</h3>
                    <button>Buy Now</button>
                </div>
            </div>
        </div>
    {{/each}}
    </div>
    
</div>
<script src="/socket.io/socket.io.js"></script>
<script src="/js/realTimeProducts.js"></script>